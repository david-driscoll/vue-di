{
    "name": "vue-di",
    "version": "0.19.7",
    "repository": {
        "type": "git",
        "url": "github.com/david-driscoll/vue-di"
    },
    "description": "Dependency injection for Vue using a derived Aurelia container",
    "main": "cjs/src/index.js",
    "module": "src/index.js",
    "browser": "",
    "types": "src/index.d.ts",
    "scripts": {
        "build": "npm run rollup:di && npm run rollup:vue && npm run rollup:vuex && npm run build:ts",
        "build:ts": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json",
        "rollup:di": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
        "rollup:vue": "rollup -c rollup-vue.config.js --environment INCLUDE_DEPS,BUILD:production",
        "rollup:vuex": "rollup -c rollup-vuex.config.js --environment INCLUDE_DEPS,BUILD:production",
        "test": "npm run build:ts && mocha",
        "prettier": "prettier **/*.ts --write",
        "lint": "tslint -p tsconfig.json --fix"
    },
    "author": "David Driscoll",
    "license": "MIT",
    "peerDependencies": {
        "reflect-metadata": "^0.1.12",
        "ts-disposables": "^2.2.3",
        "tslib": "^1.9.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@nuxt/vue-app": "^2.6.3",
        "@types/chai": "^4.1.7",
        "@types/chai-as-promised": "^7.1.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.0.0",
        "@types/rollup": "^0.54.0",
        "@types/sinon": "^7.0.11",
        "@types/sinon-chai": "^3.2.2",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "esm": "^3.2.22",
        "jsdom": "^15.0.0",
        "jsdom-global": "^3.0.2",
        "lodash": "^4.17.11",
        "mocha": "^6.1.4",
        "prettier": "^1.17.0",
        "reflect-metadata": "^0.1.13",
        "rollup": "^1.11.3",
        "rollup-plugin-typescript2": "^0.21.0",
        "sinon": "^7.3.2",
        "sinon-chai": "^3.3.0",
        "ts-disposables": "^2.2.3",
        "ts-node": "^8.1.0",
        "tslib": "^1.9.3",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5",
        "vue": "^2.6.10",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.1.1",
        "vue-template-compiler": "^2.6.10",
        "vue-test-utils": "^1.0.0-beta.9",
        "vuex": "^3.1.1",
        "vuex-module-decorators": "^0.9.9"
    }
}
