{
    "name": "vue-di",
    "version": "0.5.4",
    "repository": {
        "type": "git",
        "url": "github.com/david-driscoll/vue-di"
    },
    "description": "Dependency injection for Vue using a derived Aurelia container",
    "main": "di.cjs.js",
    "module": "di.es.js",
    "jsnext:main": "di.es.js",
    "types": "di.d.ts",
    "scripts": {
        "build": "npm run rollup:di && npm run rollup:vue",
        "rollup:di": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
        "rollup:vue": "rollup -c rollup-vue.config.js --environment INCLUDE_DEPS,BUILD:production",
        "test": "mocha",
        "prettier": "prettier **/*.ts --write",
        "lint": "tslint -p tsconfig.json --fix"
    },
    "author": "David Driscoll",
    "license": "MIT",
    "peerDependencies": {
        "reflect-metadata": "^0.1.12",
        "ts-disposables": "^2.2.3",
        "tslib": "^1.9.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/chai-as-promised": "^7.1.0",
        "@types/mocha": "^5.2.3",
        "@types/node": "^10.5.0",
        "@types/rollup": "^0.54.0",
        "@types/sinon": "^5.0.1",
        "@types/sinon-chai": "^3.2.0",
        "chai": "^4.1.2",
        "chai-as-promised": "^7.1.1",
        "jsdom": "^11.11.0",
        "jsdom-global": "^3.0.2",
        "lodash": "^4.17.10",
        "mocha": "^5.2.0",
        "prettier": "^1.13.6",
        "reflect-metadata": "^0.1.12",
        "rollup": "^0.62.0",
        "rollup-plugin-typescript2": "^0.15.1",
        "sinon": "^6.0.1",
        "sinon-chai": "^3.2.0",
        "ts-disposables": "^2.2.3",
        "ts-node": "^7.0.0",
        "tslib": "^1.9.3",
        "tslint": "^5.10.0",
        "typescript": "^2.9.2",
        "vue": "^2.5.16",
        "vue-class-component": "^6.2.0",
        "vue-property-decorator": "^7.0.0",
        "vue-template-compiler": "^2.5.16",
        "vue-test-utils": "^1.0.0-beta.9"
    }
}
