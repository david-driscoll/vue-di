{
    "name": "vue-di",
    "version": "0.14.4",
    "repository": {
        "type": "git",
        "url": "github.com/david-driscoll/vue-di"
    },
    "description": "Dependency injection for Vue using a derived Aurelia container",
    "main": "index.js",
    "module": "index.esm.js",
    "jsnext:main": "index.esm.js",
    "types": "di.d.ts",
    "scripts": {
        "build": "npm run rollup:di && npm run rollup:vue && npm run rollup:vuex && tsc -p . -outDir dist",
        "rollup:di": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
        "rollup:vue": "rollup -c rollup-vue.config.js --environment INCLUDE_DEPS,BUILD:production",
        "rollup:vuex": "rollup -c rollup-vuex.config.js --environment INCLUDE_DEPS,BUILD:production",
        "test": "mocha",
        "prettier": "prettier **/*.ts --write",
        "lint": "tslint -p tsconfig.json --fix"
    },
    "author": "David Driscoll",
    "license": "MIT",
    "peerDependencies": {
        "reflect-metadata": "^0.1.12",
        "ts-disposables": "^2.2.3",
        "tslib": "^1.9.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/chai-as-promised": "^7.1.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^11.9.5",
        "@types/rollup": "^0.54.0",
        "@types/sinon": "^7.0.6",
        "@types/sinon-chai": "^3.2.2",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "jsdom": "^13.2.0",
        "jsdom-global": "^3.0.2",
        "lodash": "^4.17.11",
        "mocha": "^6.0.1",
        "prettier": "^1.16.4",
        "reflect-metadata": "^0.1.13",
        "rollup": "^1.2.3",
        "rollup-plugin-typescript2": "^0.19.2",
        "sinon": "^7.2.4",
        "sinon-chai": "^3.3.0",
        "ts-disposables": "^2.2.3",
        "ts-node": "^8.0.2",
        "tslib": "^1.9.3",
        "tslint": "^5.13.0",
        "typescript": "^3.3.3333",
        "vue": "^2.6.7",
        "vue-class-component": "^7.0.1",
        "vue-property-decorator": "^7.3.0",
        "vue-template-compiler": "^2.6.7",
        "vue-test-utils": "^1.0.0-beta.9",
        "vuex": "^3.1.0",
        "vuex-module-decorators": "^0.9.8"
    }
}
